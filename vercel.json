{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/src/$1"
        },
        {
            "src": "/",
            "dest": "/src/index.html"
        }
    ]
}